


public class Challenge01 {
    public static int[] reverseArray(int[] inputArray) {
        int[] reversedArray = new int[inputArray.length];

        for (int i = 0; i < inputArray.length; i++) {
            reversedArray[i] = inputArray[inputArray.length - 1 - i];
        }

        return reversedArray;
    }

    public static void main(String[] args) {
        int[] inputArray = {1, 2, 3, 4, 5};

        System.out.println("Input Array: " + java.util.Arrays.toString(inputArray));

        int[] reversedArray = reverseArray(inputArray);

        System.out.println("Reversed Array: " + java.util.Arrays.toString(reversedArray));
    }
}


   
